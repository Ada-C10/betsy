<section class="main-body">



<h2>Add a Country that Categorizes the Products You Are Selling</h2>

<%= render partial: 'layouts/form_errors',
locals: {
  model: @category
}
%>

<section>
  <%= form_with model: @category do |f| %>

  <div class="form-group">
    <%= f.text_field :name, placeholder: "ex. Nicaragua", class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.submit class: "btn btn-outline-info" %>
  </div>
  <% end %>
</section>


</section>
